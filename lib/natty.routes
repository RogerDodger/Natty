GET / -> Game.list (games)

GET /login -> User.login
GET /logout -> User.logout
POST /auth -> User.auth

GET /players -> Player.list (players)

ANY / -> User.authd {
   POST /player/add -> Player.add

   GET /game/create -> Game.create
   GET /game/draw -> Game.draw

   ANY /game/:id/ -> Game.fetch {
      POST /score -> Game.score (game-score)
   }
}

